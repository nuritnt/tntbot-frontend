<% @links.each do |link| %>
<div style="margin-bottom: 15px;">
  <div class="row">
    <div class="columns small-6 small-centered" style="background-color: #E1DEE5; padding: 20px 25px 5px 25px; color:#615D5C; font-size:20px; line-height:25px; font-family: Courier New; ">
      <a name="content" href="<%= link.url %>"><%= link.title %></a>
      <p>
        <%= link.created_at.strftime('%F %T %z') %></br>
        <%= link.user_id %></br>
        <%= link.channel_id %></br>
      </p>
    </div>
  </div>
</div>
<% end %>
